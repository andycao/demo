// nib
@import 'nib'

//nib css reset
global-reset()

// test
border-radius()
  -webkit-border-radius arguments
  -moz-border-radius arguments
  border-radius arguments


redColor = #c83c2c
yellowColor = #fcf1d4
html
  font-size 100%
  
.img-inside
    
  img
    width 100%

.link-wrap
  background-color redColor
  text-align center
  padding 50px 0
  a
    img
      width 40%

  p
    padding 5px 0
    color yellowColor
  p.first
    margin-top 20px